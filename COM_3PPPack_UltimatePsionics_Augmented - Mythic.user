    <exprreq message="10th Mythic tier required"><![CDATA[#tierprereq[] >= 10]]></exprreq>
    </thing>
  <thing id="fPUMRapMet" name="Rapid Metabolism" description="{b}Benefit{/b}: When you regain hit points from daily healing, you regain all lost hit points and heal all ability damage, including any ability burn." compset="Feat" summary="You heal at incredible speed." uniqueness="useronce">
    <fieldval field="reqCon" value="13"/>
