    <eval phase="PostLevel" priority="9000"><![CDATA[
      ~ If we're disabled, do nothing
      doneif (tagis[Helper.FtDisable] <> 0)

      ~ Increase Pisonic Shot by 1
      #value[fPUPsiShot] *= 2]]></eval>
    <exprreq message="Point Blank Shot required."><![CDATA[#hasfeat[fPUPsiShot] <> 0]]></exprreq>
    </thing>
  <thing id="fPUMPsiTal" name="Psionic Talent" description="{b}Benefit{/b}: Double the number of power points gained for each Psionic Talent feat you possess." compset="Feat" summary="Your reservoir of psionic power grows more quickly than most.">
